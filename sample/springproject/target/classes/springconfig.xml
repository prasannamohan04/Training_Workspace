<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
    
    <bean id="sal" class="core.salary"> 
   <property name="codeo" value = "456"></property>
   <property name="empnameo" value = "mohan"></property>
   <property name="salaryo" value = "7890"></property>
   <property name="addr" ref= "address"></property>
   </bean> 
   <bean id ="address"  class = "core.address">
   <property name="city" value ="bhadrachalam"></property>
   <property name="pincode" value = "507111"></property>
   </bean>
  
    <!--  <constructor-arg name ="regno" value = "459"></constructor-arg>
    <constructor-arg name ="stdname" value ="james"></constructor-arg>
    -->
</beans>
